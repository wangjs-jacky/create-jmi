      "",
      "const App: React.FC = () => {",
      "  const [size, setSize] = useState<SizeType | [SizeType, SizeType] | 'customize'>('small');",
      "  const [customSize, setCustomSize] = React.useState<number>(0);",
      "  return (",
      "    <>",
      "      <Radio.Group value={size} onChange={(e) => setSize(e.target.value)}>",
